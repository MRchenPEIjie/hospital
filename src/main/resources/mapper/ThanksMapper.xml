<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xcy.mapper.ThanksMapper">

    <select id="insertThanks" parameterType="consultAndThanks" resultType="int">
        insert into consultandthanks (id,name,sex,age,phone,address,email,details,appraise) values (null,#{name},#{sex},
        #{age},#{phone},#{address},#{email}.#{details},#{appraise})
    </select>

</mapper>